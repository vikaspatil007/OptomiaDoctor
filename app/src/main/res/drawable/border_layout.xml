<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="4dp"
        />
    <gradient
        android:angle="90"
        android:centerX="35%"
        android:centerColor="#25ffffff"
        android:startColor="#25ffffff"
        android:endColor="#25ffffff"
        android:type="linear"
        />
